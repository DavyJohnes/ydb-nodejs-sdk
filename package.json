{
  "name": "@yandex-data-ui/ydb-sdk",
  "version": "0.0.1",
  "description": "Node.js bindings for working with YDB API over gRPC",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:bundle": "pbjs -t static-module -w commonjs -p . `ls kikimr/public/api/grpc/*.proto` yandex/cloud/iam/v1/iam_token_service.proto > proto/bundle.js && pbts -o proto/bundle.d.ts proto/bundle.js",
    "build": "tsc",
    "start": "node build/examples/basic-example/index.js | pino-pretty"
  },
  "keywords": [
    "ydb",
    "sdk",
    "grpc"
  ],
  "author": "Timur Sufiev",
  "license": "ISC",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "grpc": "^1.23.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "luxon": "^1.21.2",
    "moment": "^2.24.0",
    "pino": "^5.14.0",
    "protobufjs": "^6.8.8",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.144",
    "@types/luxon": "^1.21.0",
    "@types/pino": "^5.14.0",
    "pino-pretty": "^3.5.0",
    "typescript": "^3.6.4"
  }
}
